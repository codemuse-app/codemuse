{
  "name": "vscode",
  "displayName": "vscode",
  "description": "AI onboarding and peer programming.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension/index.js",
  "contributes": {},
  "scripts": {
    "gulp": "gulp",
    "vscode:prepublish": "npm run gulp && npm run compile",
    "compile": "npm run gulp && tsc -p ./",
    "watch": "npm run gulp && tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.5",
    "@types/node": "^20.9.3",
    "@types/vscode": "^1.84.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "gulp": "^4.0.2",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@sourcegraph/scip-python": "^0.6.0",
    "@sourcegraph/scip-typescript": "^0.3.9",
    "google-protobuf": "^3.21.2",
    "graphology": "^0.25.4"
  }
}
