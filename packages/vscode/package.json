{
  "name": "codemuse",
  "displayName": "CodeMuse",
  "description": "AI onboarding and peer programming.",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension/index.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codemuse",
          "title": "CodeMuse",
          "icon": "media/sidebar.svg"
        }
      ]
    },
    "views": {
      "codemuse": [
        {
          "type": "webview",
          "id": "codemuse.sidebar",
          "name": "CodeMuse"
        }
      ]
    },
    "commands": [
      {
        "command": "codemuse.openSidebar",
        "title": "Open CodeMuse sidebar"
      }
    ]
  },
  "scripts": {
    "gulp": "gulp",
    "vscode:prepublish": "npm run compile",
    "compile": "npm run gulp && webpack --mode production --devtool hidden-source-map",
    "watch": "npm run gulp && webpack --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "package": "vsce package --allow-missing-repository --no-dependencies --allow-star-activation --skip-license",
    "selfinstall": "npm run package && code --install-extension vscode-0.0.0.vsix --force"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.5",
    "@types/node": "^20.9.3",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/vscode": "^1.84.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "gulp": "^4.0.2",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@sourcegraph/scip-python": "^0.6.0",
    "@sourcegraph/scip-typescript": "^0.3.9",
    "@types/vscode-webview": "^1.57.4",
    "@vscode/vsce": "^2.22.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "google-protobuf": "^3.21.2",
    "graphology": "^0.25.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "vsce": {
    "dependencies": false
  }
}
